    "expo-sharing": "~10.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-task-manager": "~10.1.0",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-moment": "^1.1.1",
    "react-native": "0.64.3",
    "react-native-dotenv": "^2.5.5",
    "react-native-easy-toast": "^2.0.0",
    "react-native-gesture-handler": "~2.1.0",
